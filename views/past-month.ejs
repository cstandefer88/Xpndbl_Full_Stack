<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <title>Auth Template</title>
    <link href="https://fonts.googleapis.com/css?family=Oswald" rel="stylesheet">
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>

    <div>
      <div class="resultsBox">
        <h1>Form for: <%= month.date %> </h1>
        <h2>Income: $ <%= month.income %></h2>
        <div>
          <p class="line">------------------------------------------------------------</p>
          <h2>Expenses</h2>
              <% month.expenseItems.forEach(function(expenseItem) { %>
              <div>
                <h4><%= expenseItem.category %></h4>
                <ul id="expenseCategory">
                  <li class="expenseTable">$<%= expenseItem.amount %></li>
                  <li class="expenseTable"><%= expenseItem.descriptor %></li>
                </ul>
              </div>
              <% }) %>
        </div>
      </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <script src='/javascripts/bundle.js'></script>
  </body>
</html>
